//brightness
unsigned int maxBrightness;






void setup() { 
Serial.begin(9600);
FastLED.addLeds<NEOPIXEL,DATA_PIN>(leds, NUM_LEDS); 
FastLED.setBrightness(maxBrightness);
}

void loop() { 

/** The following is audio sampling function I found on the adafruit website that converts analog data 
from an electret mic to voltage readings. There's probably a better way to accurately measure analog data
but this is fine for me for now. Really hoping I can find a way to measure analog mic data to seperately identifty 
bass, mid, and high frequencies. **/
 

unsigned long startMillis= millis();  // Start of sample window
unsigned int peakToPeak = 0;   // peak-to-peak level
 
unsigned int signalMax = 0;
unsigned int signalMin = 1024;

  // collect data for 50 mS
   while (millis() - startMillis < sampleWindow)
   {
      sample = analogRead(0);
      if (sample < 1024)  // toss out spurious readings
      {
         if (sample > signalMax)
         {
            signalMax = sample;  // save just the max levels
         }
         else if (sample < signalMin)
         {
            signalMin = sample;  // save just the min levels
         }
      }
   }
   peakToPeak = signalMax - signalMin;  // max - min = peak-peak amplitude
  double volts = (peakToPeak * 5.0) / 1024;  // convert to volts

  /** if statements for animation sequences based on voltage readings from mic. 
  Set these how you like based on loudness, I just based this off my room and computer speakers.**/
   
   if (volts <= .35){
    FastLED.setBrightness(15);
    frameSpeed = 50;
    skullStill();
    
   }
   else if (volts > .35 && volts < .6){
    FastLED.setBrightness(15);
    frameSpeed = 10;
    skullBasic();
    
   }
   else if (volts > .6 && volts < 1.1 ) {
    FastLED.setBrightness(30);
    frameSpeed = 5;
    skullBasic();

   }
   else if (volts >= 1.1) {

    FastLED.setBrightness(100);
    frameSpeed = 25;
    skullColor();
    
   }
   Serial.println(volts);
 }

// animation sequence 1, Skull Still

void skullStill() { 

FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
  FastLED.setBrightness(15);
    leds[i] = pgm_read_dword(&(SkullStillLeft[i]));
  }

FastLED.show();
delay(frameSpeed);

}


// animation sequence 2, Skull Eye Roll

void skullBasic() {

// Put SkullBasic1 first frame

FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
  FastLED.setBrightness(15);
    leds[i] = pgm_read_dword(&(SkullBasic1[i]));
  }

FastLED.show();
delay(frameSpeed);


// Put SkullBasic2 second frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBasic2[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBasic3 third frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBasic3[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBasic4 fourth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBasic4[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBasic5 fifth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBasic5[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBasic6 sixth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBasic6[i]));
  }

FastLED.show();
delay(frameSpeed);
}

//animation sequence 3, explosive skull

void skullColor() {

// Put SkullBlue1 first frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBlue1[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullRed2 second frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullRed2[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBlue3 third frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBlue3[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullRed4 fourth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullRed4[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullBlue5 fifth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullBlue5[i]));
  }

FastLED.show();
delay(frameSpeed);

// Put SkullRed6 sixth frame
FastLED.clear();
for(int i = 0; i < NUM_LEDS; i++) {
    leds[i] = pgm_read_dword(&(SkullRed6[i]));
  }

FastLED.show();
delay(frameSpeed);

}
